MODULE multiple-entry-points.spvasm

BUFFER temp   16 FILL INT32 0
BUFFER output 16 FILL INT32 0
BUFFER x       4 DATA INT32 42

DESCRIPTOR_SET 0 0 0 temp
DESCRIPTOR_SET 0 1 0 output
DESCRIPTOR_SET 0 2 0 x

ENTRY foo
DISPATCH 4 1 1

ENTRY bar
DISPATCH 4 1 1

DUMP INT32 output

# CHECK: Buffer 'output' (16 bytes):
# CHECK:   output[0] = 42
# CHECK:   output[1] = 43
# CHECK:   output[2] = 44
# CHECK:   output[3] = 45
